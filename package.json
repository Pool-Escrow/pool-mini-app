{
    "name": "pool-mini",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "check-types": "tsc --noEmit",
        "start": "next start",
        "lint": "next lint --fix",
        "gen:contracts": "wagmi generate -c wagmi-cli.config.ts",
        "deploy:mainnet": "./scripts/deploy_pool.sh mainnet",
        "deploy:sepolia": "./scripts/deploy_pool.sh sepolia",
        "deploy:local": "./scripts/deploy_pool.sh local",
        "deploy:droplet-mainnet": "./scripts/deploy_droplet.sh"
    },
    "dependencies": {
        "@coinbase/onchainkit": "0.38.13",
        "@farcaster/frame-sdk": "0.0.41",
        "@radix-ui/react-dialog": "1.1.13",
        "@radix-ui/react-dropdown-menu": "2.1.14",
        "@radix-ui/react-label": "2.1.6",
        "@radix-ui/react-slot": "1.2.2",
        "@t3-oss/env-nextjs": "0.13.4",
        "@tanstack/react-query": "5.76.1",
        "@upstash/redis": "1.34.9",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "lucide-react": "0.510.0",
        "next": "15.3.2",
        "next-themes": "0.4.6",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "sonner": "2.0.3",
        "tailwind-merge": "3.3.0",
        "vaul": "1.1.2",
        "viem": "2.29.2",
        "wagmi": "2.15.3",
        "zod": "3.24.4"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.3.1",
        "@eslint/js": "9.26.0",
        "@tailwindcss/postcss": "4.1.6",
        "@tanstack/eslint-plugin-query": "5.74.7",
        "@testing-library/react": "16.3.0",
        "@types/bun": "1.2.13",
        "@types/jest": "29.5.14",
        "@types/node": "22.15.18",
        "@types/react": "19.1.4",
        "@types/react-dom": "19.1.5",
        "@typescript-eslint/eslint-plugin": "8.32.1",
        "@typescript-eslint/parser": "8.32.1",
        "@wagmi/cli": "2.3.1",
        "critters": "0.0.25",
        "eslint": "9.26.0",
        "eslint-config-next": "15.3.2",
        "eslint-config-prettier": "10.1.5",
        "eslint-plugin-storybook": "0.12.0",
        "eslint-plugin-tailwindcss": "3.18.0",
        "prettier": "3.5.3",
        "prettier-plugin-tailwindcss": "0.6.11",
        "tailwindcss": "4.1.6",
        "tw-animate-css": "1.2.9",
        "typescript": "5.8.3",
        "typescript-eslint": "8.32.1",
        "vitest": "3.1.3",
        "webpack": "5.99.8"
    },
    "overrides": {
        "tailwindcss": "4.1.6",
        "react": "19.1.0"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "endOfLine": "lf",
        "jsxSingleQuote": true,
        "printWidth": 120,
        "proseWrap": "preserve",
        "quoteProps": "consistent",
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all",
        "useTabs": false,
        "tabWidth": 4,
        "plugins": [
            "prettier-plugin-tailwindcss"
        ],
        "tailwindFunctions": [
            "clsx",
            "classnames",
            "cn",
            "cva",
            "tw",
            "twMerge"
        ]
    },
    "trustedDependencies": [
        "@tailwindcss/oxide",
        "bufferutil",
        "esbuild",
        "keccak",
        "sharp",
        "unrs-resolver",
        "utf-8-validate"
    ]
}
